{
  "name": "@resonance/backend",
  "version": "0.0.1",
  "description": "Resonance backend API server",
  "type": "module",
  "main": "./dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
