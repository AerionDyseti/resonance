{
  "name": "@resonance/api",
  "version": "0.0.1",
  "description": "Resonance tRPC API (inbound adapter) + contract",
  "private": true,
  "type": "module",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "bun": "./src/index.ts",
      "import": "./src/index.ts",
      "types": "./src/index.ts"
    },
    "./trpc": {
      "bun": "./src/trpc/contract.ts",
      "import": "./src/trpc/contract.ts",
      "types": "./src/trpc/contract.ts"
    }
  },
  "scripts": {
    "type-check": "tsc --noEmit",
    "build": "tsc --noEmit",
    "test": "bun test --pass-with-no-tests"
  },
  "dependencies": {
    "@resonance/core": "*",
    "@resonance/shared": "*",
    "@trpc/server": "^11.7.2",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@types/bun": "^1.2.16",
    "typescript": "^5.9.3"
  }
}
